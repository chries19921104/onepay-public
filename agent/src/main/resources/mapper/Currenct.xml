<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.agent.mapper.SystemCurrencyMapper">
    <select id="selectCurrency" resultType="String">
        select currency
        from system_currency
        where is_active = 1
    </select>

</mapper>