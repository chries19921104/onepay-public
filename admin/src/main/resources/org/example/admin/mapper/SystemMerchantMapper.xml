            <result property="foTpiDriver" column="FO_Tpi_driver" jdbcType="VARCHAR"/>
            <result property="checkAccname" column="check_accname" jdbcType="TINYINT"/>
            <result property="payFoBankFee" column="pay_fo_bank_fee" jdbcType="TINYINT"/>
            <result property="settlementTerm" column="settlement_term" jdbcType="TINYINT"/>
