<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.admin.mapper.SystemMerchantMapper">

    <resultMap id="BaseResultMap" type="org.example.common.entity.SystemMerchant">
            <id property="merchantId" column="merchant_id" jdbcType="BIGINT"/>
            <result property="cardGroupId" column="card_group_id" jdbcType="BIGINT"/>
            <result property="agentId" column="agent_id" jdbcType="BIGINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="code" column="code" jdbcType="VARCHAR"/>
            <result property="secCode" column="sec_code" jdbcType="VARCHAR"/>
            <result property="name4qr" column="name4qr" jdbcType="VARCHAR"/>
            <result property="currency" column="currency" jdbcType="VARCHAR"/>
            <result property="website" column="website" jdbcType="VARCHAR"/>
            <result property="trRate" column="tr_rate" jdbcType="DECIMAL"/>
            <result property="trQrRate" column="tr_qr_rate" jdbcType="DECIMAL"/>
            <result property="trTrueRate" column="tr_true_rate" jdbcType="DECIMAL"/>
            <result property="trLocalbankRate" column="tr_localbank_rate" jdbcType="DECIMAL"/>
            <result property="wdRate" column="wd_rate" jdbcType="DECIMAL"/>
            <result property="wdRateAmount" column="wd_rate_amount" jdbcType="DECIMAL"/>
