        fi_true_min,fi_localbank_max,fi_localbank_min,
        fo_max,fo_min,fx_max,
        fx_min,sett_card_max,status,
        no_qr_adj,no_qr_adj_random,localbank_adj,
        localbank_adj_random,use_qr_pairing_code,VNPAY_enabled,
        FO_Tpi_driver,check_accname,pay_fo_bank_fee,
        settlement_term,created_man,updated_man,
        created_at,updated_at
    </sql>

    <select id="selectListMap" resultType="java.util.Map">
        select merchant_id,name from system_merchant where merchant_id in (#{set});
    </select>
</mapper>
