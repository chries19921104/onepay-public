    <sql id="Base_Column_List">
        merchant_id,card_group_id,agent_id,
        name,code,sec_code,
        name4qr,currency,website,
        tr_rate,tr_qr_rate,tr_true_rate,
        tr_localbank_rate,wd_rate,wd_rate_amount,
        sett_fee,fi_bank_max,fi_bank_min,
