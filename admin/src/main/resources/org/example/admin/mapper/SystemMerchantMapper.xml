            <result property="fiQrpayMax" column="fi_qrpay_max" jdbcType="DECIMAL"/>
            <result property="fiQrpayMin" column="fi_qrpay_min" jdbcType="DECIMAL"/>
            <result property="fiTrueMax" column="fi_true_max" jdbcType="DECIMAL"/>
            <result property="fiTrueMin" column="fi_true_min" jdbcType="DECIMAL"/>
            <result property="fiLocalbankMax" column="fi_localbank_max" jdbcType="DECIMAL"/>
            <result property="fiLocalbankMin" column="fi_localbank_min" jdbcType="DECIMAL"/>
            <result property="foMax" column="fo_max" jdbcType="DECIMAL"/>
            <result property="foMin" column="fo_min" jdbcType="DECIMAL"/>
